-- Tải DiscordLib thư viện với toàn bộ cú pháp
thành công cục bộ, DiscordLib = pcall(function()
    trả về loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord"))()
kết thúc)
nếu không thành công thì
    Warn("Không thể tải DiscordLib!")
    trở lại
kết thúc

chiến thắng cục bộ = DiscordLib:Window("Pop hub")

------------------------------
-- Máy chủ: ⭐️Meme Adventure⭐️
------------------------------
máy chủ cục bộ = win:Server("⭐️Meme Adventure⭐️", "")
thả cục bộ = serv:Kênh("Chính")

-- Giết khí
drops:Button("giết aura", function()
    getgenv().G = đúng
    getgenv().Creator = 'https://discord.gg/B3HqPPzFYr - HalloweenGaster'
    sinh ra(hàm()
        trong khi getgenv().G và getgenv().Creator == 'https://discord.gg/B3HqPPzFYr - HalloweenGaster' làm
            chờ đợi(0.9)
            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", 112412400000)
            sethiddenproperty(game.Players.LocalPlayer, "MaxSimulationRadius", 112412400000)
            đối với _, d theo cặp (game.Workspace:GetDescendants()) thì
                nếu d:IsA("Humanoid") và d.Parent.Name ~= game.Players.LocalPlayer.Name thì
                    d. Sức khỏe = 0
                kết thúc
            kết thúc
        kết thúc
    kết thúc)
kết thúc)

-- Tắt hào quang giết chóc
drops:Button("tắt sát khí", function()
    getgenv().G = sai
    getgenv().Creator = 'https://discord.gg/B3HqPPzFYr - HalloweenGaster'
    -- Nếu cần xử lý thêm, bạn có thể đặt thêm mã tại đây
kết thúc)

-- Năng suất Inf
thả: Nút("inf yield", hàm()
    loadstring(trò chơi:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
kết thúc)

