------------------------------
-- Kênh: câu cá
------------------------------
cục bộ dropsFishing = serv:Channel("câu cá")
hàm cục bộ startFishing(remotePath, eventName)
    getgenv().FishingEnabled = đúng
    getgenv().Creator = 'https://discord.gg/B3HqPPzFYr - HalloweenGaster'
    đối số cục bộ = { [1] = "Câu cá1" }
    sinh ra(hàm()
        trong khi getgenv().FishingEnabled và getgenv().Creator == 'https://discord.gg/B3HqPPzFYr - HalloweenGaster' làm
            -- Use pcall để tránh lỗi nếu object chưa sẵn sàng
            pcall(hàm()
                remotePath.RemoteEvent:FireServer(giải nén(đối số))
            kết thúc)
            chờ đợi(0.1)
        kết thúc
    kết thúc)
kết thúc

dropsFishing:Button("câu cá sa mạc", function()
    startFishing(game:GetService("Người chơi").LocalPlayer.Character.Fishing.Desert, "Fishing1")
kết thúc)

dropsFishing:Button("câu cá dung nham", function()
    startFishing(game:GetService("Người chơi").LocalPlayer.Character.Fishing.Lava, "Fishing1")
kết thúc)

dropsFishing:Button("câu cá tuyết", hàm()
    startFishing(game:GetService("Người chơi").LocalPlayer.Character.Fishing.Snow, "Fishing1")
kết thúc)

-- Tự động nhấp vào yêu cầu đá quý
dropsFishing:Button("tự động nhấp vào yêu cầu đá quý", hàm()
    getgenv().G = đúng
    getgenv().Creator = 'https://discord.gg/B3HqPPzFYr - HalloweenGaster'
    người chơi cục bộ = trò chơi.Người chơi.Người chơi cục bộ
    ký tự cục bộ = player.Character hoặc player.CharacterAdded:Wait()
    ký tự:WaitForChild("HumanoidRootPart")
    hàm cục bộ activatePrompts()
        đối với _, nhắc trong ipairs(workspace:GetDescendants()) thực hiện
            nếu nhắc: IsA("ProximityPrompt") thì
                dấu nhắc cục bộPos = nil
                nếu prompt.Parent và prompt.Parent:IsA("Model") và prompt.Parent.PrimaryPart thì
                    promptPos = prompt.Parent.PrimaryPart.Position
                elseif prompt.Parent và prompt.Parent:IsA("BasePart") thì
                    promptPos = prompt.Parent.Position
                khác
                    pcall(hàm() promptPos = prompt.Parent.Position kết thúc)
                kết thúc
                nếu promptPos thì
                    khoảng cách cục bộ = (character.HumanoidRootPart.Position - promptPos).Độ lớn
                    nếu khoảng cách <= prompt.MaxActivationDistance thì
                        fireproximityprompt(nhắc nhở)
                        print("Đã kích hoạt ProximityPrompt: " .. nhắc.Name)
                    kết thúc
                kết thúc
            kết thúc
        kết thúc
    kết thúc
    sinh ra(hàm()
        trong khi getgenv().G và getgenv().Creator == 'https://discord.gg/B3HqPPzFYr - HalloweenGaster' làm
            kích hoạtPrompts()
            chờ đợi(0.5)
        kết thúc
    kết thúc)
kết thúc)

dropsFishing:Button("tắt yêu cầu gem", function()
    getgenv().G = sai
    getgenv().Creator = 'https://discord.gg/B3HqPPzFYr - HalloweenGaster'
kết thúc)

dropsFishing:Button("tắt tất cả các câu cá", function()
    getgenv().FishingEnabled = sai
kết thúc)

