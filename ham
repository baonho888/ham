    trò chơi: GetService("Người chơi").LocalPlayer.PlayerGui.QuestTake.Accept3.RemoteEvent:FireServer()
kết thúc
kết thúc)

cục bộ dropsRandom = serv2:Channel("bí mật ngẫu nhiên")
dropsRandom:Button("nếu tập lệnh không hoạt động, hãy đến hòn đảo đó", function()
    -- Không có hành động nào
kết thúc)

dropsRandom:Button("biển bí mật 1", hàm()
    sinh ra(hàm()
        trong khi đúng là làm
            pcall(hàm()
                fireclickdetector(game:GetService("Workspace").MAP["ĐẢO STARTER [Lv 1+ ]"]["Bí mật ngẫu nhiên"].NGẪU NHIÊN)
            kết thúc)
            chờ đợi(0.1)
        kết thúc
    kết thúc)
kết thúc)

dropsRandom:Button("biển bí mật 2", hàm()
    sinh ra(hàm()
        trong khi đúng là làm
            pcall(hàm()
                fireclickdetector(game:GetService("Workspace").MAP["ĐẢO STARTER [Lv 1+ ]"]["Bí mật ngẫu nhiên"].NGẪU NHIÊN)
            kết thúc)
            chờ đợi(0.1)
        kết thúc
    kết thúc)
kết thúc)

dropsRandom:Button("yorus v3", hàm()
    sinh ra(hàm()
        trong khi đúng là làm
            clickDetector cục bộ = game.Workspace:FindFirstChild("MAP")
                và trò chơi.Workspace.MAP:FindFirstChild("ĐẢO DAK [Cấp độ 1350+]")
                và trò chơi.Workspace.MAP["ĐẢO DAK [Lv 1350+]"]:FindFirstChild("Yoru V3 ngẫu nhiên")
                và trò chơi.Workspace.MAP["ĐẢO DAK [Cấp độ 1350+]"]["Yoru V3 ngẫu nhiên"]:FindFirstChild("ClickDetector")
            nếu clickDetector thì
                máy dò nhấp chuột (clickDetector)
            khác
                Warn("ClickDetector không tìm thấy!")
            kết thúc
            chờ đợi(0.1)
        kết thúc
    kết thúc)
kết thúc)

dropsRandom:Button("wando sea 2", function()
    sinh ra(hàm()
        trong khi đúng là làm
            clickDetector cục bộ = game.Workspace:FindFirstChild("Thị trấn Bạch kim [ 2000 - 3000 ]")
                và trò chơi.Workspace["Platinum Town [ 2000 - 3000 ]"]:FindFirstChild("PlatinumMAP")
                và trò chơi.Workspace["Platinum Town [ 2000 - 3000 ]"].PlatinumMAP:FindFirstChild("GodBox")
                và trò chơi.Workspace["Platinum Town [ 2000 - 3000 ]"].PlatinumMAP.GodBox:FindFirstChild("ClickDetector")
            nếu clickDetector thì
                máy dò nhấp chuột (clickDetector)
            khác
                Warn("ClickDetector không tìm thấy!")
            kết thúc
            chờ đợi(0.1)
        kết thúc
    kết thúc)
kết thúc)

thả cục bộ = serv2:Kênh("dịch chuyển tức thời")

giọt: Nút("biển 1👇", hàm()
kết thúc)

