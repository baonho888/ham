cục bộ dropsArmors = serv2:Channel("armors")
dropsArmors:Button("sao chép toàn bộ giáp của sv", function()
    Người chơi cục bộ = trò chơi:GetService("Người chơi")
    giáp địa phương = {"Áo giáp bị nguyền rủa", "Áo giáp độc nhất", "Áo giáp bóng tối", "Áo giáp sấm sét", "Áo giáp sấm sét", "Áo giáp kim cương", "Áo giáp vàng", "Áo giáp sử thi", "Áo giáp sắt", "Áo giáp gỗ"}
    đối với _, người chơi theo cặp (Players:GetPlayers()) làm
        ba lô cục bộ = player:FindFirstChild("Ba lô")
        nếu ba lô thì
            đối với _, armorName theo cặp(áo giáp) làm
                giáp cục bộ = ba lô:FindFirstChild(armorName)
                nếu giáp và giáp:FindFirstChild("K") và giáp.K:FindFirstChild("Fire") thì
                    áo giáp.K.Fire:FireServer()
                kết thúc
            kết thúc
        kết thúc
    kết thúc
kết thúc)

